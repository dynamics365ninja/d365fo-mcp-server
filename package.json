{
  "name": "d365fo-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for X++ Code Completion in D365 Finance & Operations",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "extract-metadata": "tsx scripts/extract-metadata.ts",
    "build-database": "tsx scripts/build-database.ts",
    "index-metadata": "tsx scripts/extract-metadata.ts && tsx scripts/build-database.ts"
  },
  "keywords": [
    "mcp",
    "x++",
    "dynamics365",
    "d365fo",
    "code-completion"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.5.0",
    "@azure/storage-blob": "^12.26.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "better-sqlite3": "^11.7.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "fast-xml-parser": "^4.5.1",
    "ioredis": "^5.9.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^6.0.2",
    "@types/node": "^22.10.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
